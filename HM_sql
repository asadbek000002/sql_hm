"A"

select id, person_name, city from employee, works
where employee.id = works.id and company_name = 'FirstBank Corporation'

"B"

select id, person_name, city, from employee, works
where employee.id = works.id and company_name = 'FirstBank Corporation' and salary > 10000

"C"

select id from employe, works
where employee.id = works.id and company_name <> 'FirstBank Corporation'
